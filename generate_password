import random
import string

def generate_password(length=12, level="medium"):
    chars = string.ascii_letters
    if level == "medium":
        chars += string.digits
    elif level == "high":
        chars += string.digits + string.punctuation

    return "".join(random.choice(chars) for _ in range(length))

if __name__ == "__main__":
    print("Easy:   ", generate_password(8, "easy"))
    print("Medium: ", generate_password(12, "medium"))
    print("High:   ", generate_password(16, "high"))
